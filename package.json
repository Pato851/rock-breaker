{
  "name": "my-habit-rock",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "pack:docs": "repomix --include \"docs/**\" --output repomix-docs.xml",
    "pack:src": "repomix --include \"src/**\" --output repomix-src.xml",
    "pack:all": "repomix --output repomix-all.xml",
    "format:docs": "prettier --write \"docs/*.md\" \"README.md\"",
    "lint": "eslint .",
    "lint:docs": "prettier --check \"docs/*.md\" \"README.md\""
  },
  "dependencies": {
    "vue": "^3.5.24"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "eslint": "^9.39.2",
    "eslint-plugin-vue": "^10.6.2",
    "globals": "^17.0.0",
    "prettier": "^3.7.4",
    "repomix": "^1.11.0",
    "vite": "npm:rolldown-vite@7.2.5"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.2.5"
  }
}